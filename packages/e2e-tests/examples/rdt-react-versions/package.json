    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "workspaces": [
    "react-15-app",
    "react-16-app",
    "react-17-app"
